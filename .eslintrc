
{
  "parser": "babel-eslint",

  "extends": [
    "airbnb-base",
    "prettier"
  ],

  "plugins": [
    "prettier"
  ],

  "env": {
    "es6": true,
    "node": true
  },

  "rules": {
    "prettier/prettier": "error",
    "import/no-unresolved": ["error", { ignore: ["electron"] }],
    "import/extensions": ["off", "never"],
    "import/resolver": {
      "node": {
        "extensions": [".js"]
      }
    },
    "no-shadow": "off"
  }
}
